<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE params SYSTEM "params.dtd">


<params name="ResearcherMenu">
  <section name="dynamic controls">
    <section name="CheckboxTaskLaneKeeping">
      <attstr name="type" val="check box"/>
      <attstr name="text" val="Lane keeping"/>
      <attstr name="tip" val="Select the Lane Keeping task"/>
      <attnum name="x" val="20"/>
      <attnum name="y" val="370"/>
      <attnum name="image width" val="20"/>
      <attnum name="image height" val="20"/>
      <attstr name="color" val="0xFFFFFF"/>
      <attstr name="font" val="small_t"/>
      <attstr name="checked" val="yes"/>
    </section>

    <section name="CheckboxTaskSpeedControl">
      <attstr name="type" val="check box"/>
      <attstr name="text" val="Speed control"/>
      <attstr name="tip" val="Select the Speed Control task"/>
      <attnum name="x" val="20"/>
      <attnum name="y" val="340"/>
      <attnum name="image width" val="20"/>
      <attnum name="image height" val="20"/>
      <attstr name="color" val="0xFFFFFF"/>
      <attstr name="font" val="small_t"/>
      <attstr name="checked" val="no"/>
