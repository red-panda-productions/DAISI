<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-cavallo-360rb" type="template" mode="mw">
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" val="0.05"/>
		<attnum name="avoid width" val="0.25"/>
		<attnum name="scale brake" val="0.8"/>
		<attnum name="scale bump" val="0.01"/>
		<attnum name="scale bump outer" val="0.01"/>
		<attnum name="scale mu" val="1.00"/>
		<attnum name="side mu" val="0.90"/>
	</section>
</params>
