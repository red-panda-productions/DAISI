<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-cavallo-360rb" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" val="10"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" val="18"/>
	</section>
	<section name="Rear Differential">
		<attstr name="type" val="LIMITED SLIP"/>
		<attnum name="ratio" min="0" max="10" val="3.65"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" val="0.55"/>
		<attnum name="max pressure" unit="kPa" val="40000"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="1000"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="1000"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" val="0.03"/>
		<attnum name="avoid width" val="0.25"/>
		<attnum name="scale brake" val="0.9"/>
		<attnum name="scale bump" val="0.38"/>
		<attnum name="scale bump outer" val="0.6"/>
		<attnum name="scale mu" val="0.95"/>
	</section>
</params>


