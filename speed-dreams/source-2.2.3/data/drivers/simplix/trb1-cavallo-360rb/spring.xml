<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-cavallo-360rb" type="template" mode="mw">
	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="0" max="10" val="3.33"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" val="0.6"/>
		<attnum name="max pressure" unit="kPa" val="40000"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="2000"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="0"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" val="4000"/>
	</section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" val="0.05"/>
		<attnum name="avoid width" min="0.0" max="3.0" val="0.25"/>
		<attnum name="scale brake" min="0" max="1" val="0.8"/>
		<attnum name="scale bump" min="0" max="1" val="0.25"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.4"/>
		<attnum name="scale mu" min="0" max="2" val="0.90"/>
		<attnum name="side mu" min="0" max="2" val="0.90"/>
	</section>
</params>
