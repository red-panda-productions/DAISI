<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-cavallo-360rb" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" val="1"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" val="1.5"/>
	</section>
	<section name="Rear Differential">
		<attstr name="type" val="LIMITED SLIP"/>
		<attnum name="ratio" val="3.33"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" val="0.6"/>
		<attnum name="max pressure" unit="kPa" val="40000"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="2000"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" val="1500"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" val="6000"/>
		<attnum name="bellcrank" val="1.05"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" val="6000"/>
		<attnum name="bellcrank" val="1.05"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" val="4500"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" val="4500"/>
	</section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" val="0.10"/>
		<attnum name="scale brake" val="0.95"/>
		<attnum name="scale bump" val="0.1"/>
		<attnum name="scale bump outer" val="0.2"/>
		<attnum name="scale mu" val="1.00"/>
		<attnum name="side mu" val="0.90"/>
	</section>
</params>
