			</section>
			<section name="16">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="17">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="18">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="19">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
