				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="6">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="7">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="8">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="9">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="10">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="11">
				<attnum name="rpm" unit="rpm" min="0" max="20000" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="2000.0"/>
			</section>
			<section name="12">
