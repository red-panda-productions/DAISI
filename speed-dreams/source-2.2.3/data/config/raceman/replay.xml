<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE params SYSTEM "params.dtd">


<params name="Replay" version="1.23">
  <section name="Header">
    <attstr name="name" val="Replay"/>
    <attstr name="type" val="Replay"/>
    <attstr name="description" val="Replay"/>
    <attnum name="priority" val="90.000000"/>
  </section>

  <section name="Tracks">
    <attnum name="maximum number" val="1.000000"/>
    <section name="1">
      <attstr name="name" val="test_highway"/>
      <attstr name="category" val="road"/>
    </section>

  </section>

  <section name="Races">
    <section name="1">
      <attstr name="name" val="Replay"/>
    </section>

  </section>

  <section name="Replay">
    <attstr name="time of day" val="afternoon"/>
    <attstr name="display mode" val="results only"/>
    <section name="Starting Grid">
      <attnum name="rows" val="2.000000"/>
      <attnum name="distance to start" val="25.000000"/>
      <attnum name="distance between columns" val="20.000000"/>
      <attnum name="offset within a column" val="10.000000"/>
      <attnum name="initial speed" val="0.000000"/>
      <attnum name="initial height" val="0.200000"/>
    </section>

  </section>

  <section name="Drivers">
    <attnum name="maximum number" val="1.000000"/>
    <attstr name="focused module" val="replaydriver"/>
    <attnum name="focused idx" val="1.000000"/>
    <attstr name="rejected types" val="networkhuman"/>
    <section name="1">
      <attnum name="idx" val="1.000000"/>
      <attstr name="module" val="replaydriver"/>
      <attnum name="extended" val="0.000000"/>
      <attnum name="skin targets" val="0.000000"/>
    </section>

  </section>

</params>
